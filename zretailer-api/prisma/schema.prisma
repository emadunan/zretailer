// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id           Int     @id @default(autoincrement())
  title        String
  category     String
  desc         String?
  pkgCap       Int
  pkgPriceBuy  Int
  pkgPriceSell Int
  unitPrice    Int
}
